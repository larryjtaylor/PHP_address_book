 <html>
    <head>
        <link rel='stylesheet' href='/../css/bootstrap.css' type='text/css'>
        <link rel='stylesheet' href='/../css/styles.css' type='text/css'>
        <title>Contacts</title>
    </head>
    <body>
        <div class='jumbotron'>
            <div class='row'>
                <div class='col-md-6'>
                    <img src='/../img/telBook.png' id='telBook' />
                    <h1 class='header' id='address'>Address</h1>
                </div>
                <div class='col-md-6'>
                    <h1 class='header' id='book'>Book</h1>
                    <img src='/../img/rolodex.png' id='rolodex'>
                </div>
            </div>
        </div>
        <div class='well'>
            <h1 class=displayHeader>Here is your contact:</h1>
            <p class='contactName'>{{ newContacts.getName }}</p>
            <ul>
                <li>{{ newContacts.getPhone }}</li><br>
                <li>{{ newContacts.getStreet }}</li><br>
                <li>{{ newContacts.getLocation }}</li><br>
            </ul>
            <p class='button'><a href='/'>View your contacts</a></p>
        </div>
    </body>
</html>
